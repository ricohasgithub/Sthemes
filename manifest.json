{
    "name": "Sthemes",
    "version": "1.0",
    "description": "Smart web page theme displayer",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_title": "sth",
        "default_popup": "src/html/popup.html"
    },
    "permissions": ["storage", "unlimitedStorage", "tabs", "<all_urls>"]
}
